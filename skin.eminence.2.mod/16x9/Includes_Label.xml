<includes>

    <variable name="Label_Title">
        <value condition="Container.Content(episodes) + !IsEmpty(ListItem.Title)">$INFO[ListItem.Title]</value>
        <value>$INFO[ListItem.Label]</value>
    </variable>

    <variable name="Label_Year">
        <value condition="Container.Content(tvshows) + !IsEmpty(ListItem.LastPlayed)">$LOCALIZE[31731] $INFO[ListItem.LastPlayed]</value>      
        <value condition="Container.Content(episodes) + !IsEmpty(ListItem.Premiered)">$LOCALIZE[31732] $INFO[ListItem.Premiered]</value>       
        <value condition="!IsEmpty(ListItem.Year) + !Container.Content(episodes) + !Container.Content(tvshows)">$INFO[ListItem.Year]</value>
    </variable>

<!-- Ajout ICI -->
    <variable name="Label_PostersList1">
        <value condition="Container.Content(artists)">$INFO[ListItem.Artist]</value>
